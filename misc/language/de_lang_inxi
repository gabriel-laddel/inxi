#### Print Output

L_PRINT_SHORT_DATA_LINE_1="${C1}%s$cpc_plural${CN}[${C2}${SEP1}${cpu_data_string} ${cpu_model}$model_plural (${cpu_type}) %s ${min_max_clock}${SEP1}${CN}] ${C1}%s${CN}[${C2}${SEP1}${current_kernel}${SEP1}${CN}] ${C1}%s${CN}[${C2}${SEP1}${up_time}${SEP1}${CN}] ${C1}%s${CN}[${C2}${SEP1}${memory}${SEP1}${CN}] ${C1}%s${CN}[${C2}${SEP1}${hdd_capacity}($hdd_used)${SEP1}${CN}] ${C1}%s${CN}[${C2}${SEP1}${processes}${SEP1}${CN}]"
L_PRINT_SHORT_DATA_LINE_2="${short_data} ${C1}%s${CN}[${C2}${SEP1}${IRC_CLIENT}${IRC_CLIENT_VERSION}${SEP1}${CN}]"
